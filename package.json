{
  "name": "wakatime-chrome-parcel",
  "version": "1.0.0",
  "description": "WakaTime chrome extension built with parcel",
  "main": "index.js",
  "scripts": {
    "dev": "parcel src/react/index.html",
    "build": "NODE_ENV=production parcel build src/react/popup.html src/react/events.js -d dist/ --no-cache --public-url /dist/",
    "watch-react": "parcel watch src/react/popup.html src/react/events.js -d dist/ --no-cache --public-url /dist/",
    "watch": "parcel watch src/popup.html src/options.html src/devtools.html src/js/events.js --public-url /dist/ --no-cache"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "less": "^3.8.1",
    "node-sass": "^4.9.3",
    "parcel-bundler": "^1.9.7"
  },
  "dependencies": {
    "@atlaskit/banner": "^7.0.5",
    "@atlaskit/button": "^9.0.7",
    "@atlaskit/css-reset": "^3.0.1",
    "@atlaskit/dropdown-menu": "^6.1.10",
    "@atlaskit/empty-state": "^3.0.6",
    "@atlaskit/field-text-area": "^4.0.8",
    "@atlaskit/global-navigation": "^4.2.0",
    "@atlaskit/navigation-next": "^3.6.2",
    "@atlaskit/page": "^8.0.4",
    "@atlaskit/toggle": "^5.0.7",
    "axios": "^0.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "bootstrap": "^4.1.3",
    "classnames": "^2.2.6",
    "emotion": "^9.2.6",
    "jquery": "^3.3.1",
    "moment": "^2.22.2",
    "popper.js": "^1.14.3",
    "react": "16.4.2",
    "react-addons-css-transition-group": "^15.6.2",
    "react-dom": "^16.4.2",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-transition-group": "^2.4.0",
    "styled-components": "^3.4.5",
    "webextension-polyfill": "^0.3.1",
    "webextensions-api-fake": "^0.5.1"
  }
}
